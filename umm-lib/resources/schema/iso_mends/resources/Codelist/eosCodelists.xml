   xsi:schemaLocation="http://www.isotc211.org/2005/gmx ../../gmx/gmx.xsd http://www.isotc211.org/2005/gco ../../gco/gco.xsd http://www.opengis.net/gml ../../gml/gml.xsd http://www.w3.org/1999/xlink ../../xlink/xlinks.xsd">
   <gmx:name>
      <gco:CharacterString>eosCodelists</gco:CharacterString>
   </gmx:name>
   <gmx:scope>
      <gco:CharacterString>Codelists for description of NASA Additional
         Attributes</gco:CharacterString>
   </gmx:scope>
   <gmx:fieldOfApplication>
      <gco:CharacterString>EOS Namespace</gco:CharacterString>
   </gmx:fieldOfApplication>
   <gmx:versionNumber>
      <gco:CharacterString>0.0</gco:CharacterString>
   </gmx:versionNumber>
