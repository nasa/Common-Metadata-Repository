      <gco:Date>2013-04-21</gco:Date>
   </gmx:versionDate>
   <gmx:codelistItem>
      <gmx:CodeListDictionary gml:id="EOS_AdditionalAttributeTypeCode">
         <gml:description>Codes that describe types used to classify NASA/ESDIS additional attributes.</gml:description>
         <gml:identifier codeSpace="gov.nasa.esdis">EOS_AdditionalAttributeTypeCode</gml:identifier>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_geographicIdentifier">
               <gml:description>The Additional Attribute provides an identifier for a geographic
                  feature.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">geographicIdentifier</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_qualityInformation">
               <gml:description>The Additional Attribute provides information about quality of the
                  resource.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">qualityInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_instrumentInformation">
               <gml:description>The Additional Attribute provides information about
                  instrument(s).</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">instrumentInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_sensorInformation">
               <gml:description>The Additional Attribute provides information about
                  sensor(s).</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">sensorInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_contentInformation">
               <gml:description>The Additional Attribute provides information about the content of
                  the resource.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">contentInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_platformInformation">
               <gml:description>The Additional Attribute provides information about
                  platform(s).</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">platformInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_citation.identifier">
               <gml:description>The Additional Attribute provides an identifier for the
                  resource.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">citation.identifier</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_descriptiveKeyword">
               <gml:description>The Additional Attribute provides a keyword value.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">descriptiveKeyword</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_processingInformation">
               <gml:description>The Additional Attribute provides information about processing of
                  the resource(s).</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">processingInformation</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
        <gmx:codeEntry>
          <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_processingParameter">
            <gml:description>The Additional Attribute provides information about a parameter used in processing of
              the resource(s).</gml:description>
            <gml:identifier codeSpace="gov.nasa.esdis">processingParameter</gml:identifier>
          </gmx:CodeDefinition>
        </gmx:codeEntry>
        <gmx:codeEntry>
          <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeTypeCode_commandLineArgument">
            <gml:description>The Additional Attribute provides information about a commandLineArgument used in processing of
              the resource(s).</gml:description>
            <gml:identifier codeSpace="gov.nasa.esdis">commandLineArgument</gml:identifier>
          </gmx:CodeDefinition>
        </gmx:codeEntry>
      </gmx:CodeListDictionary>
   </gmx:codelistItem>
   <gmx:codelistItem>
      <gmx:CodeListDictionary gml:id="EOS_AdditionalAttributeDataTypeCode">
        <gml:description>Codes that describe basic computer science types for NASA/ESDIS additional attribute values.</gml:description>
         <gml:identifier codeSpace="gov.nasa.esdis">EOS_AdditionalAttributeDataTypeCode</gml:identifier>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_string">
               <gml:description>The parameter value type is XML primitive string type. See the XML
                  Schema specification (http://www.w3.org/TR/xmlschema-2/#string) for more
                  information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">string</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_float">
               <gml:description>The parameter value type is XML primitive float type, without
                  support for positive or negative infinity, or not-a-number. See the XML Schema
                  specification (http://www.w3.org/TR/xmlschema-2/#string) for more
                  information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">float</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_int">
               <gml:description>The parameter value type is XML derived int type. See the XML Schema
                  specification (http://www.w3.org/TR/xmlschema-2/#string) for more
                  information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">int</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_boolean">
               <gml:description>The parameter value type is XML primitive boolean type. See the XML
                  Schema specification (http://www.w3.org/TR/xmlschema-2/#string) for more
                  information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">boolean</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_date">
               <gml:description>The parameter value type is XML primitive date type but with GMT
                  timezone, meaning any timezoned or non-timezoned value will be treated as having
                  GMT timezone. See the XML Schema specification
                  (http://www.w3.org/TR/xmlschema-2/#string) for more information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">date</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_time">
               <gml:description>The parameter value type is XML primitive time type but with GMT
                  timezone, meaning any timezoned or non-timezoned value will be treated as having
                  GMT timezone. See the XML Schema specification
                  (http://www.w3.org/TR/xmlschema-2/#string) for more information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">time</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_dateTime">
               <gml:description>The parameter value type is XML primitive dateTime type. See the XML
                  Schema specification (http://www.w3.org/TR/xmlschema-2/#string) for more
                  information.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">dateTime</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_dateString">
               <gml:description>The parameter value type is a string that follows a provider-defined
                  date format.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">dateString</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_timeString">
               <gml:description>The parameter value type is a string that follows a provider-defined
                  time format.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">timeString</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AdditionalAttributeDataTypeCode_dateTimeString">
               <gml:description>The parameter value type is a string that follows a provider-defined
                  dateTime format.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis">dateTimeString</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
      </gmx:CodeListDictionary>
   </gmx:codelistItem>
   <!--=== DS_AssociationTypeCode ===-->
   <gmx:codelistItem>
      <gmx:CodeListDictionary gml:id="EOS_AssociationTypeCode">
         <gml:description>justification for the correlation of two datasets</gml:description>
         <gml:identifier codeSpace="gov.nasa.esdis.umm">EOS_AssociationTypeCode</gml:identifier>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_childDataset">
               <gml:description>reference from one dataset to another</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Child_Dataset</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_collaborativeOtherAgency">
               <gml:description>reference to a master dataset of which this one is a part</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Collaborative_Other_Agency</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_fieldCampaign">
               <gml:description>part of the same structured set of data held in a computer</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Field_Campaign</gml:identifier>
         </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_parentDataset">
               <gml:description>mapping and charting information from which the dataset content originates</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Parent_Dataset</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_relatedDataset">
               <gml:description>part of a set of imagery that when used together, provides three-dimensional images</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Related_Dataset</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_other">
               <gml:description>part of a set of larger works that is identified by a digital object identifier.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Other</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_isPreviousVersionOf">
               <gml:description>This collection has been superseded by the Associated DOI.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Is_Previous_Version_Of</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_isNewVersionOf">
               <gml:description>The collection supersedes the Associated DOI. </gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">Is_New_Version_Of</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
         <gmx:codeEntry>
            <gmx:CodeDefinition gml:id="EOS_AssociationTypeCode_doiPreviousVersion">
               <gml:description>part of a set of larger works that is identified by a digital object identifier.</gml:description>
               <gml:identifier codeSpace="gov.nasa.esdis.umm">DOI_Previous_Version</gml:identifier>
            </gmx:CodeDefinition>
         </gmx:codeEntry>
      </gmx:CodeListDictionary>
   </gmx:codelistItem>
</gmx:CT_CodelistCatalogue>
