              <xs:maxLength value="1024" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="S3CredentialsAPIDocumentationURL">
          <xs:annotation>
            <xs:documentation>Defines the URL where the credential documentation are stored.
            </xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:anyURI">
              <xs:minLength value="1" />
              <xs:maxLength value="1024" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>

